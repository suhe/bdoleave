    	SELECT DATE_FORMAT(balanced.leave_date,'%d/%m/%Y') as leave_date,balanced.leave_description,Balanced.leave_balance_total,Balanced.source,Balanced.balance
    	FROM
    	(
    	SELECT
    	leave_balance_date as leave_date,leave_balance_description as leave_description,leave_balance_total,source,@saldo:=@saldo+leave_balance_total AS balance
    	FROM
    	(SELECT leave_balance_date leave_balance_date,leave_balance_stype,leave_balance_description,+leave_balance_total,'Leaves' as source
    	FROM leave_balance
    	WHERE employee_id = 317
    	UNION ALL
    	SELECT leave_date leave_date,'2' as leave_balance_stype,CONCAT(leave_description,' (',leave_range,')') leave_description,-leave_total,IF(leave_source=1,'Timesheet','Leaves') as source
    	FROM `leaves`
    	WHERE employee_id = 317 and leave_approved = 1
    	) balance
    	JOIN (SELECT @saldo:=0) a
    	ORDER BY balance.leave_balance_date,balance.leave_balance_stype DESC
    	) AS balanced
    	WHERE balanced.leave_date <> '' ";i:1;i:80;i:2;s:21:"yii\db\Command::query";i:3;d:1454932545.614521026611328125;i:4;a:2:{i:0;a:5:{s:4:"file";s:48:"/app/www/bdoleave/views/report/balanced-card.php";s:4:"line";i:113;s:8:"function";s:6:"widget";s:5:"class";s:15:"yii\base\Widget";s:4:"type";s:2:"::";}i:1;a:5:{s:4:"file";s:50:"/app/www/bdoleave/controllers/ReportController.php";s:4:"line";i:61;s:8:"function";s:6:"render";s:5:"class";s:19:"yii\base\Controller";s:4:"type";s:2:"->";}}}i:40;a:5:{i:0;s:1016:"
