    	) AS balanced
    	WHERE balanced.leave_date <> '' ";i:1;i:80;i:2;s:21:"yii\db\Command::query";i:3;d:1454933878.6945970058441162109375;i:4;a:2:{i:0;a:5:{s:4:"file";s:48:"/app/www/bdoleave/views/report/balanced-card.php";s:4:"line";i:107;s:8:"function";s:6:"widget";s:5:"class";s:15:"yii\base\Widget";s:4:"type";s:2:"::";}i:1;a:5:{s:4:"file";s:50:"/app/www/bdoleave/controllers/ReportController.php";s:4:"line";i:62;s:8:"function";s:6:"render";s:5:"class";s:19:"yii\base\Controller";s:4:"type";s:2:"->";}}}i:37;a:5:{i:0;s:1138:"
